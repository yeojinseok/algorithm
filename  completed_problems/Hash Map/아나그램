function solution(str1, str2) {
  let answer = 'YES'
  const strMap1 = new Map()
  for (let x of str1) {
    if (strMap1.has(x)) strMap1.set(x, strMap1.get(x) + 1)
    else strMap1.set(x, 1)
  }
  for (let x of str2) {
    if (!strMap1.has(x) || strMap1.get(x) == 0) return 'NO'
    strMap1.set(x, strMap1.get(x) - 1)
  }
  return answer
}
